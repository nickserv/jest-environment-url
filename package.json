{
  "name": "jest-environment-url",
  "version": "0.1.7",
  "repository": "nickmccurdy/jest-environment-url",
  "funding": "https://github.com/sponsors/nickmccurdy",
  "keywords": [
    "jest",
    "jsdom",
    "testing"
  ],
  "author": "Nick McCurdy <nick@nickmccurdy.com> (https://nickmccurdy.com/)",
  "license": "ISC",
  "description": "Load a URL into JSDOM for testing",
  "main": "dist",
  "types": "dist",
  "scripts": {
    "prepare": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@jest/environment": "^29.0.1",
    "@jest/types": "^29.0.1",
    "@types/jsdom": "^20.0.0",
    "jest-mock": "^29.0.1",
    "jest-util": "^29.0.1",
    "jsdom": "^20.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^28.1.6",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "prettier-config-nick": "^1.0.3",
    "typescript": "^4.7.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "testEnvironment": "./index.ts",
    "testEnvironmentOptions": {
      "url": "https://example.com/"
    },
    "testPathIgnorePatterns": [
      "dist"
    ]
  },
  "prettier": "prettier-config-nick"
}
